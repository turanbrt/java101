import java.util.Scanner;

public class Fibonacci {
    public static void main(String[] args) {
        int n,a=0,b=1,c;
        Scanner inp=new Scanner(System.in);
        System.out.println("Fibonacci serisi i√ßin eleman sayisi giriniz: ");
        n=inp.nextInt();
        System.out.print(a+" ");
        System.out.print(b+" ");
        for(int i=0;    i<n-1;    i++){
            c=a+b;
            System.out.print(c+" ");
            a=b;
            b=c;

        }

    }
}
