import java.util.Scanner;

public class hesap {
    public static void main(String[] args) {
        int sayi;
        double toplam=0;
        Scanner inp=new Scanner(System.in);

        System.out.print("Sayi giriniz: ");
        sayi=inp.nextInt();

        for(int i=1; i<=sayi;  i++){
            toplam=toplam+(1.0/i);
        }
        System.out.println("Toplam: "+toplam);

    }
}
