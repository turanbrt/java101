
public class asal {
    public static void main(String[] args) {

        for(int i=1; i<100; i++){
            int j=1;
            boolean asal=true;
            do{
                if(i%j==0 && j!=1){
                    asal=false;
                }
                j++;
            }while (j<i );

            if(asal){
                System.out.print(i+"-");
            }
        }
    }
}
